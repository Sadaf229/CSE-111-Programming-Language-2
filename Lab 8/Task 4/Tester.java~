public class Tester {
  public static void main(String[] args) {
    RealNumber rn = new ComplexNumber();
    System.out.println(rn);
    
    System.out.println(&quot;-- -- -- -- -- -- -- -- -- -- &quot;);
    
    rn = new ComplexNumber(5, 7);
    System.out.println(rn);
    
    System.out.println(&quot;-- -- -- -- -- -- -- -- -- -- &quot;);
    ComplexNumber cn = new ComplexNumber();
    cn.check();
  }
}